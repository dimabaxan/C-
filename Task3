using System;

class Program
{
    static void Main(string[] args)
    {
        int[] myArray = { 1, 2, 3, 4, 5 };
        PrintArrayReverse(myArray, myArray.Length - 1);
    }

    static void PrintArrayReverse(int[] arr, int index)
    {
        if (index < 0)
            return;

        Console.WriteLine(arr[index]);
        PrintArrayReverse(arr, index - 1);
    }
}
